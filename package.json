  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/sasjakoning/real-time-web-2021/issues"
  },
  "homepage": "https://github.com/sasjakoning/real-time-web-2021#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.2",
    "express-session": "^1.17.3",
    "http": "^0.0.1-security",
    "node-fetch": "^3.3.1",
    "socket.io": "^4.6.1"
  },
